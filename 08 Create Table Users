CREATE TABLE Users
(
Id BIGINT IDENTITY(1,1) PRIMARY KEY,
Username VARCHAR(30) UNIQUE NOT NULL,
Password VARCHAR (26) UNIQUE NOT NULL,
ProfilePicture VARBINARY(max),
LastLoginTime DATE,
IsDeleted BIT
)

